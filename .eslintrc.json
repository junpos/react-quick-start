{
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": [2, "never"],
    "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0
  }
}
